<br>
<table>
	<th>Booking Number</th>
	<th>Flight number</th>
	<th>Name</th>
	<th>Email</th>
	<% passenger.each do |pax| %>
		<tr>
			<td><%= booking.id %></tdd>
			<td><%= flight.number %></td>
			<td><%= link_to pax.name, passenger_path(pax.id) %></td>
			<td><%= pax.email %></td>
		</tr>
	<% end %>
</table>
